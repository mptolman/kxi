class Foo
{
    private Bar bar;

    Foo()
    {}

    public Bar bar()
    {
        return bar;
    }
}

class Bar
{
    private A a;

    Bar()
    {}

    public A a()
    {
        return a;
    }
}

class A
{
    public int x;
    public int y;

    public int add(int x, int y)
    {
        return x + y;
    }
}

void main()
{
    int x;
    int y;
}