class A {
    public int x;
    public int y;
    
    A()
    {
    }
    
    public int factorial(int x)
    {
        if (x <= 1)
            return 1;
        else
            return x * factorial(x-1);
    }
}

void main() 
{
    A a = new A();
    a.x = 1;
    a.y = 2;
    cout << a.x + a.y;
    cout << '\n';
}
